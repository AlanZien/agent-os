# Notion Integration
# Get your token from: https://www.notion.so/my-integrations
NOTION_TOKEN=ntn_your_notion_integration_token_here

# Notion Database IDs
# Find these in your Notion database URLs
# Example URL: https://notion.so/workspace/abc123def456...
# Database ID: abc123def456
NOTION_PROJECTS_DB_ID=your_projects_database_id_here
NOTION_TASKS_DB_ID=your_tasks_database_id_here

# Optional: Notion Bugs Database (if you use bug tracking)
NOTION_BUGS_DB_ID=your_bugs_database_id_here
